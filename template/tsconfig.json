{
  "compilerOptions": {
    "allowJs": true,
    "module": "esnext",
    "experimentalDecorators": true,
    "target": "es6",
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "moduleResolution": "node",
    "removeComments": false,
    "sourceMap": true,
    "strictFunctionTypes": false,
    "baseUrl": "./"
  },
  "include": [
    "./src/**/*",
  ],
  "exclude": [
    "./template/**/*",
  ]
}
